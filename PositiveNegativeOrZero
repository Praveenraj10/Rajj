import java.util.Scanner;

public class PositiveNegativeOrZero {
    public static void main(String arg[]){
        Scanner sc=new Scanner(System.in);
        int num=sc.nextInt();
        String result="";
        if(num>0){
            result="Positive";
        }
        else if(num<0){
            result="Negative";
        }
        else{
            result="Zero";
        }
        System.out.println(result);
    }
}
